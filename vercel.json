{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "express",
  "functions": {
    "dist/index.js": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/index.html",
      "status": 200
    }
  ]
}
